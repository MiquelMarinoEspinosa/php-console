{
  "name": "php/console",
  "license": "free",
  "description": "Php Console",
  "keywords": [
    "Php",
    "console"
  ],
  "authors": [
    {
      "name": "php-console",
      "email": "php@console.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Php\\Console\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Php\\Console\\Tests\\Unit\\": "tests/unit/",
      "Php\\Console\\Tests\\Integration\\": "tests/integration/",
      "Php\\Console\\Tests\\Acceptance\\": "tests/acceptance/"
    }
  },
  "require": {
    "ramsey/uuid": "^4.1",
    "symfony/framework-bundle": "^5.1",
    "symfony/console": "^5.1",
    "symfony/yaml": "^5.1",
    "symfony/monolog-bundle": "^3.5"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "behat/behat": "^3.7",
    "phpunit/phpunit": "^9.3",
    "phpspec/prophecy-phpunit": "^2.0"
  }
}
