{
    "name": "php/console",
    "license": "free",
    "description": "Php Console",
    "keywords": ["Php","console"],
    "authors": [
        {
            "name": "php-console",
            "email": "php@console.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Php\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Php\\Console\\Tests\\Unit\\": "tests/unit/",
            "Php\\Console\\VideosImporter\\Tests\\Integration\\": "tests/integration/",
            "Php\\Console\\VideosImporter\\Tests\\Acceptance\\": "tests/acceptance/"
        }
    },
    "require": {
        "hirak/prestissimo": "^0.3.8",
        "ramsey/uuid": "^3.8",
        "symfony/framework-bundle": "^4.1",
        "symfony/console": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3",
        "behat/behat": "^3.5"
    }
}
